<%= form_tag('/listings', method:"post") do %>
  <%= label_tag(:image_url, "Image URL:") %>
  <%= text_field_tag(:image_url) %>
  <%= label_tag(:address, "Address: ") %>
  <%= text_field_tag(:address) %>
  <%= label_tag(:neighborhood, "Neighborhood: ") %>
  <%= text_field_Tag(:neighborhood) %>
  <%= label_tag(:price_per_night, "Price per night") %>
  <%= number_field_tag(:price_per_night) %>
  <%= label_tag(:description, "Description: ") %> 
  <%= text_area_tag(:description) %>
  <%= submit_tag("Add Your Listing") %>
<% end %> 